<h1>List of Users</h1>


<table border=1>
  <tbody>
    <th>
      <td>Name</td>
      <td>PD:</td>
      <td>Nausea</td>
      <td>Happiness</td>
      <td>Tickets</td>
      <td>Heights</td>
    </th>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.password_digest %></td>
      <td><%= user.nausea %></td>
      <td><%= user.happiness %></td>
      <td><%= user.tickets %></td>
      <td><%= user.height %></td>
    </tr>
    <% end %>
  </tbody>
</table>
